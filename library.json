    {
      "owner": "etlcpp",
      "name": "Embedded Template Library",
      "version": "*"
    },
    {
      "owner": "chrisjoyce911",
      "name": "esp32FOTA",
      "version": "*"
    },
    {
      "owner": "jnthas",
      "name": "Improv WiFi Library",
      "version": "^0.0.4"
    },
    {
      "name": "ArduinoTrace",
      "version": "https://github.com/hoeken/ArduinoTrace.git"
    }
  ],
  "export": {
    "include": [
      "examples/platformio",
      "src",
      "html",
      "scripts",
      "library.json",
      "library.properties",
      "LICENSE",
      "README.md"
    ]
  }
