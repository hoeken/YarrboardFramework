      "name": "ArduinoJson",
      "version": "^7.0.4"
    },
    {
      "owner": "hoeken",
      "name": "PsychicHttp",
      "version": "^2.1.1"
    },
    {
      "owner": "elims",
      "name": "PsychicMqttClient",
