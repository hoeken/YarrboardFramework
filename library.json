{
  "name": "YarrboardFramework",
  "version": "2.2.1",
  "description": "Framework for esp32 with http, websockets, MQTT and more",
  "keywords": "network,http,https,tcp,ssl,tls,websocket,espasyncwebserver",
  "repository": {
    "type": "git",
    "url": "https://github.com/hoeken/YarrboardFramework"
  },
  "authors": [
    {
      "name": "Zach Hoeken",
      "email": "hoeken@gmail.com",
      "maintainer": true
    }
  ],
  "license": "MPL-2.0",
  "examples": [
    {
      "name": "platformio",
      "base": "examples/platformio",
      "files": [
        "main.cpp"
      ]
    }
  ],
  "frameworks": "arduino",
  "platforms": "espressif32",
  "dependencies": [
